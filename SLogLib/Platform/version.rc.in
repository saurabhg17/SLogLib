				VALUE "OriginalFilename", "SLogLib"
				VALUE "ProductName",      "SLogLib"
				
				VALUE "CompanyName",      SLOGLIB_VER_COMPANYNAME_STR
				VALUE "FileDescription",  SLOGLIB_VER_FILEDESCRIPTION_STR
				VALUE "FileVersion",      SLOGLIB_VER_FILEVERSION_STR
				VALUE "OriginalFilename", SLOGLIB_VER_ORIGINALFILENAME_STR
				VALUE "ProductName",      SLOGLIB_VER_PRODUCTNAME_STR
				VALUE "ProductVersion",   SLOGLIB_VER_PRODUCTVERSION_STR
				VALUE "LegalCopyright",   SLOGLIB_LEGALCOPYRIGHT_STR
				
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252

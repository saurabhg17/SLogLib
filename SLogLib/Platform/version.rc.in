	#define SLOGLIB_LIB_FULLNAME "SlogLib.dll\0"
#endif

#define SLOGLIB_VER_COMPANYNAME_STR     "Nuts\0"
#define SLOGLIB_VER_FILEDESCRIPTION_STR "SLogLib is a c++ logging library\0"

#define SLOGLIB_VER_FILEVERSION          @SLOGLIB_VERSION_MAJOR@,@SLOGLIB_VERSION_MINOR@,0,@SLOGLIB_VERSION_BUGFIX@
#define SLOGLIB_VER_FILEVERSION_STR      "@SLOGLIB_VERSION_MAJOR@,@SLOGLIB_VERSION_MINOR@,0,@SLOGLIB_VERSION_BUGFIX@\0"

#define SLOGLIB_VER_PRODUCTVERSION       @SLOGLIB_VERSION_MAJOR@,@SLOGLIB_VERSION_MINOR@,0,@SLOGLIB_VERSION_BUGFIX@
#define SLOGLIB_VER_PRODUCTVERSION_STR   "@SLOGLIB_VERSION_MAJOR@,@SLOGLIB_VERSION_MINOR@,0,@SLOGLIB_VERSION_BUGFIX@\0"

#define SLOGLIB_LEGALCOPYRIGHT_STR       "Copyright (c) 2018 Saurabh Garg\0"
